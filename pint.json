{
  "$schema": "https://raw.githubusercontent.com/laravel/pint/main/resources/schema.json",
  "preset": "laravel",
  "rules": {
    "declare_strict_types": true,
    "no_unused_imports": true,
    "ordered_imports": {
      "sort_algorithm": "alpha"
    },
    "binary_operator_spaces": {
      "default": "single_space"
    }
  }
}
